<!DOCTYPE html>
<html>
<head>
  <title>Sample</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= render 'layouts/header' %>
<div class="content">
	<div class="container">
		<div class="row">
			<div class="col-md-2">
				<%= render 'layouts/content' %>
			</div>
			<div class="col-md-10">
				<%= yield %>
			</div>
		</div>
	</div>
</div>
<%= render 'layouts/footer' %>
</body>
</html>
